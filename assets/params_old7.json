	"ui": [ 
		{
			"type": "vgroup",
			"label":"Stereo Tester",
			"items": [ 
				{
					"type": "hslider",
					"label": "freq",
					"address": "/StereoTester/freq",
					"meta": [
						{ "1": ""},
						{ "scale": "log"},
						{ "unit": "Hz"}
					],
					"init": "440",
					"min": "40",
					"max": "20000",
					"step": "1"
				},
				{
					"type": "hslider",
					"label": "volume",
					"address": "/StereoTester/volume",
					"meta": [
						{ "2": ""},
						{ "unit": "dB"}
					],
					"init": "-10",
					"min": "-96",
					"max": "0",
					"step": "1"
				},
				{
			"type": "hgroup",
			"label":"Stereo Tester",
			"items": [ 
				{
					"type": "vslider",
					"label": "signal",
					"address": "/StereoTester/signal",
					"meta": [
						{ "3": ""},
						{ "style": "menu{'white noise':0;'pink noise':1;'sine':2}"}
					],
					"init": "0",
					"min": "0",
					"max": "2",
					"step": "1"
				},
				{
					"type": "hslider",
					"label": "channel",
					"address": "/StereoTester/channel",
					"meta": [
						{ "4": ""},
						{ "style": "radio{'none':0;'left':1;'right':2;'both':3}"}
					],
					"init": "3",
					"min": "0",
					"max": "3",
					"step": "1"
				}]}
			]
		}
